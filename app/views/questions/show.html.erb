<%= form_with model: [@test, @question], local: true do |form| %>
  <p>
    <%= form.label :question %>:
    <%= @question.body %>
  </p>

  <p>
    <%= render partial: 'shared/test_question_deleter',
               locals: { test: @test, question: @question } %>
  </p>
<% end %>

<%= link_to 'Back', test_path(@test) %>